    interface-peer {e1 n5}
    interface-peer {e2 n6}
}

node n3 {
    type pc
    network-config {
	hostname pc1-1
	!
	interface eth0
	 mac address 42:00:aa:00:00:00
	 ip address 172.16.1.1/24
	!
	interface lo0
	 type lo
	 ip address 127.0.0.1/8
	 ipv6 address ::1/128
	!
	ip route 172.16.2.0/24 172.16.1.254
	!
    }
    canvas c0
    iconcoords {48 336}
    labelcoords {48 367}
    interface-peer {eth0 n1}
}

