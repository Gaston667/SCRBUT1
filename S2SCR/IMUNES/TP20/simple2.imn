node n0 {
    type host
    network-config {
	hostname GW
	!
	interface eth1
	 mac address 42:00:aa:00:00:03
	 ip address 172.16.2.254/24
	!
	interface eth0
	 mac address 42:00:aa:00:00:02
	 ip address 172.16.1.254/24
	!
	interface lo0
	 type lo
	 ip address 127.0.0.1/8
	 ipv6 address ::1/128
	!
    }
    auto_default_routes enabled
    canvas c0
    iconcoords {432 72}
    labelcoords {432 108}
    interface-peer {eth0 n1}
    interface-peer {eth1 n2}
}

node n1 {
    type lanswitch
    network-config {
	hostname switch1
	!
    }
    canvas c0
    iconcoords {192 192}
    labelcoords {192 215}
    interface-peer {e0 n3}
    interface-peer {e1 n4}
    interface-peer {e2 n0}
}

node n2 {
